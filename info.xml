<?xml version="1.0"?>
<extension key="pilnetreports" type="module">
  <file>pilnetreports</file>
  <name>PILnet reports</name>
  <description>Custom reports for pilnet.org</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/JoineryHQ/civicrm-pilnetreports</url>
    <url desc="Documentation">https://github.com/JoineryHQ/civicrm-pilnetreports</url>
    <url desc="Support">http://joineryhq.com</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2023-03-20</releaseDate>
  <version>1.0.3</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>Developed by Joinery for PILnet.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Pilnetreports</namespace>
    <format>23.02.1</format>
    <angularModule>crmPilnetreports</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
